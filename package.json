{
  "name": "test-app",
  "version": "0.1.0",
  "private": true,
  "description": "Test Platform - Production Ready",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:e2e": "jest src/__tests__/integration --testTimeout=30000",
    "prepare": "husky",
    "setup:env": "node scripts/setup-env.js",
    "setup:admin": "node scripts/setup-admin.js",
    "setup:email": "node scripts/setup-email.js",
    "db:push": "prisma db push",
    "db:generate": "prisma generate",
    "postinstall": "prisma generate",
    "test:local": "./scripts/test-all-local.sh",
    "test:quick": "./scripts/test-quick.sh",
    "cleanup:deps": "rm -rf node_modules package-lock.json && npm install",
    "check:todos": "grep -r 'TODO\\|FIXME\\|HACK\\|XXX' --include='*.js' --include='*.jsx' --include='*.ts' --include='*.tsx' src/ || echo 'No TODOs found'",
    "check:quality": "npm run lint && npm run check:todos"
  },
  "dependencies": {
    "@fullcalendar/core": "^6.1.17",
    "@fullcalendar/daygrid": "^6.1.17",
    "@fullcalendar/interaction": "^6.1.17",
    "@fullcalendar/react": "^6.1.17",
    "@fullcalendar/timegrid": "^6.1.17",
    "@google-cloud/vision": "^5.2.0",
    "@prisma/client": "^5.10.2",
    "@tailwindcss/forms": "^0.5.9",
    "@types/bcryptjs": "^2.4.6",
    "@types/jspdf": "^1.3.3",
    "@types/multer": "^1.4.12",
    "@types/nodemailer": "^6.4.17",
    "@types/papaparse": "^5.3.16",
    "bcryptjs": "^3.0.2",
    "classnames": "^2.5.1",
    "core-js": "^3.43.0",
    "devtools-detect": "^4.0.2",
    "dotenv": "^16.5.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.3.3",
    "highlight.js": "^11.11.1",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "lucide-react": "^0.512.0",
    "mic-check": "^1.1.0",
    "multer": "^2.0.1",
    "nanoid": "^5.1.5",
    "network-speed": "^2.1.1",
    "next": "^15.3.3",
    "next-auth": "^4.24.11",
    "nodemailer": "^6.10.1",
    "papaparse": "^5.5.3",
    "pg": "^8.16.0",
    "pg-boss": "^9.0.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "recharts": "^2.15.3",
    "recordrtc": "^5.6.2",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "sass": "^1.89.1",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.17",
    "xlsx-js-style": "^1.2.0",
    "zod": "^3.25.56",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.52.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/recordrtc": "^5.6.14",
    "autoprefixer": "^10.4.21",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.2",
    "postcss": "^8.5.4",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "prisma": "^5.10.2",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write"
    ]
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "overrides": {
    "@testing-library/react": {
      "react": "$react",
      "react-dom": "$react-dom"
    }
  }
}
